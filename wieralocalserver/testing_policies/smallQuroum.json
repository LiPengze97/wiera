{
  "id": "quorum_demo",
  "host_list": {
    "dell": {
      "storage_tiers": [
        {
          "tier_name": "disk-1",
          "tier_size": "2KB",
          "tier_type": 2,
          "tier_location": "disk-1",
          "tier_expected_latency": 10,
          "default": true
        }
      ],
      "events": [
        {
          "event_type": "ActionPut",
          "event_trigger": [
            "ActionPut"
          ],
          "event_conditions": {},

          "responses": [

            {
              "response_type": "consistency.QuorumConsistency",
              "response_parameters": {
                "write_quorum":2
              }
            }
          ]
        },
        {
          "event_type": "ActionGet",
          "event_trigger": [
            "ActionGet"
          ],
          "event_conditions": {},
          "responses": [
            {
              "response_type": "consistency.QuorumConsistency",
              "response_parameters": {
                "read_quorum":2
              }
            }
          ]
        }
      ]
    },
    "linux-vm-4": {
      "storage_tiers": [
        {
          "tier_name": "disk-1",
          "tier_size": "2KB",
          "tier_type": 2,
          "tier_location": "disk-1",
          "tier_expected_latency": 10,
          "default": true
        }
      ],
      "events": [
        {
          "event_type": "ActionPut",
          "event_trigger": [
            "ActionPut"
          ],
          "event_conditions": {},

          "responses": [
            {
              "response_type": "Store",
              "response_parameters": {
                "to": ":disk-1"
              }
            },
            {
              "response_type": "consistency.QuorumConsistency",
              "response_parameters": {
                "write_quorum":2
              }
            }
          ]
        },
        {
          "event_type": "ActionGet",
          "event_trigger": [
            "ActionGet"
          ],
          "event_conditions": {},
          "responses": [
            {
              "response_type": "consistency.QuorumConsistency",
              "response_parameters": {
                "read_quorum":2
              }
            }
          ]
        }
      ]
    }
  }
}